package UIPodsistem;

import java.io.File;

import com.fasterxml.jackson.databind.ObjectMapper;

import Model.Korisnik;

public class KorisnikUI {

	public KorisnikUI() {}
	
	public void upisiKorisnika(Korisnik k) {
		ObjectMapper mapper = new ObjectMapper();
		mapper.writeValue(new File("Korisnici.json"), k);;
	}
	
	
}

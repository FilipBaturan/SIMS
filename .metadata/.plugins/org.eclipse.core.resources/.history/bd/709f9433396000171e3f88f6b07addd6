package UIPodsistem;

import java.io.File;

import com.fasterxml.jackson.databind.ObjectMapper;

import Model.Klub;

public class KluboviUI {

	public KluboviUI() {}
	
	public void upisiKlub(Klub k){
		ObjectMapper mapper = new ObjectMapper();
		mapper.writeValue(new File("Klubovi.json"),  true);
		
	}
}

package Model;

import java.text.DateFormat;
import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;

import UIPodsistem.SmestanjeIgraca;

public class Main {

	public static void main(String[] args) throws ParseException {
		new Aplikacija();
		Aplikacija.dodajKorisnika("aa", "aa");
		Aplikacija.dodajKorisnika("bb", "bb");
		Aplikacija.dodajKorisnika("cc", "cc");

		Aplikacija.dodajKlub("KLUB1");
		Aplikacija.dodajKlub("KLUB2");
		Aplikacija.dodajKlub("KLUB3");
		Aplikacija.dodajKlub("KLUB4");

		String dat = "16.11.1996.";
		DateFormat format = new SimpleDateFormat("dd.MM.yyyy.");
		Date datum = format.parse(dat);
		Osoba o = new Osoba(1, "Aleksandar", "Stefanovic", datum);
		Igrac i = new Igrac( "Aleksandar", "Stefanovic", datum)
		SmestanjeIgraca.upisiIgraca(i);
	}

}

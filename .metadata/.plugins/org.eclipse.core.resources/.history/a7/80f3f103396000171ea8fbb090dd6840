package Model;

import java.util.ArrayList;
import java.util.Date;

public class Trener extends Osoba {
	
	private Klub klub;
	private ArrayList<UcinakTrenera> ucinak;
	
	public Trener(){}

	public Trener(int id, String ime, String prezime, Date datumRodjenja) {
		super(id, ime, prezime, datumRodjenja);
		
	}

	public Klub getKlub() {
		return klub;
	}

	public void setKlub(Klub klub) {
		this.klub = klub;
	}

	public ArrayList<UcinakTrenera> getUcinak() {
		return ucinak;
	}

	public void setUcinak(ArrayList<UcinakTrenera> ucinak) {
		this.ucinak = ucinak;
	}
	
	
	
	
}

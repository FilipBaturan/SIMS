package Model;

import java.util.ArrayList;

import GUI.PrijavljivanjeDijalog;

public class Aplikacija {
	
	ArrayList<Korisnik> listaKorisnika = new ArrayList<Korisnik>(); 
	
	
	public Aplikacija() {
		new PrijavljivanjeDijalog();
	}

	
	public boolean pronadjiKorisnika(String korisnickoImeTekst, String lozinkaTekst)
	{
		for (Korisnik korisnik : listaKorisnika) {
			
			if( (korisnik.getKorisnickoIme().compareTo(korisnickoImeTekst) == 0) &&
					korisnik.getLozinka().compareTo(lozinkaTekst) == 0)
				
		}
		
		return false;
	}
			
}
